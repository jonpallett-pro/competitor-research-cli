{
  "name": "competitor-research-cli",
  "version": "1.0.0",
  "description": "CLI tool and web app for competitive research and analysis using AI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "compete": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.cli.json",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts",
    "dev:web": "next dev",
    "build:web": "next build",
    "start:web": "next start",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist .next"
  },
  "keywords": [
    "competitive-analysis",
    "cli",
    "ai",
    "market-research",
    "nextjs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "axios": "^1.7.9",
    "bottleneck": "^2.19.5",
    "chalk": "^5.4.1",
    "cheerio": "^1.0.0",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "next": "^14.2.21",
    "ora": "^8.1.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
